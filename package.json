{
  "name": "ds-bot",
  "version": "1.0.2",
  "description": "DisTube music bot.",
  "homepage": "https://github.com/distubejs/example#readme",
  "bugs": {
    "url": "https://github.com/distubejs/example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/distubejs/example.git"
  },
  "license": "MIT",
  "author": "Skick",
  "main": "src/index.ts",
  "scripts": {
    "postinstall": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "p": "prettier . --write",
    "start": "tsx src/index.ts",
    "start:dev": "tsx --watch src/index.ts",
    "update:distube": "rimraf node_modules/distube && yarn install --update-checksums --check-files"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@distube/soundcloud": "^1.3.0",
    "@distube/spotify": "^1.4.5",
    "@distube/yt-dlp": "^1.1.3",
    "discord.js": "^14.7.1",
    "distube": "file:./local_modules/distube",
    "eslint-config-prettier": "^8.5.0",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.10",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "overrides": {
    "@distube/soundcloud": {
      "distube": "$distube"
    },
    "@distube/spotify": {
      "distube": "$distube"
    },
    "@distube/yt-dlp": {
      "distube": "$distube"
    }
  },
  "resolutions": {
    "distube": "file:local_modules/distube"
  },
  "packageManager": "yarn@1.22.19",
  "nano-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  }
}
